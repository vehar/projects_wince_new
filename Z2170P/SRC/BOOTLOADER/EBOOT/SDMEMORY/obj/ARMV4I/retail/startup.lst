   40 00000000          BUILDTTB 
   41 00000000                  ; find out CHIP id, use chip id to determine what DDR is used 
   42 00000000                  ; This approach has following assumptions: 
   43 00000000                  ; 1. OMAP35xx uses Micron DDR that has 128M connected to CS0  
   44 00000000                  ;     and another 128M connected to CS1, if BSP_SDRAM_BANK1_ENABLE 
   45 00000000                  ;     is enabled. 
   46 00000000                  ; 2. 37xx uses Hynix DDR that has 256M continuous memory connected to CS0 
   47 00000000                  ; 
   48 00000000          ;       ldr     r0, =OMAP_IDCODE_REGS_PA 
   49 00000000          ;       ldr     r1, [r0] 
   50 00000000          ;       ldr     r2, =0x0FFFF000 
   51 00000000          ;       ldr     r3, =0x0B891000 
   52 00000000          ;       and             r1, r1, r2 
   53 00000000          ;       cmp             r1, r3 
   54 00000000          ;       addne   r11, pc, #g_oalAddressTable-(.+8)                       ;X Pointer to OEMAddressTable. 
   55 00000000          ;       addeq   r11, pc, #g_oalAddressTableHynix - (. + 8)      ;V Pointer to OEMAddressTable for Hynix device 
   56 00000000 e28fbf59         add             r11, pc, #g_oalAddressTableHynix - (. + 8) 
   57 00000004                   
   58 00000004              ; Set the TTB. 
   59 00000004              ; 
   60 00000004 e59f91f0     ldr     r9, =BSP_PTES_PA 
   61 00000008 e59f01f0     ldr     r0, =0xFFFFC000                   ; 
   62 0000000c e0099000     and     r9, r9, r0                        ; Mask off TTB to be on 16K boundary. 
   63 00000010 ee029f10     mcr     p15, 0, r9, c2, c0, 0             ; Set the TTB. 
   64 00000014            
   65 00000014              ; ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
   66 00000014              ; ~~~~~~~~~~ MAP CACHED and BUFFERED SECTION DESCRIPTORS ~~~~~~~~~~~~~~~~~~ 
   67 00000014              ; ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
   68 00000014 e3a0000a     mov     r0, #0x0A                         ; Section (1MB) descriptor; (C=1 B=0: cached write-through). 
   69 00000018 e3800b01     orr     r0, r0, #0x400                    ; Set AP. 
   70 0000001c e1a0100b 20  mov     r1, r11                           ; Pointer to OEMAddressTable. 
   71 00000020            
   72 00000020              ; Start Crunching through the OEMAddressTable[]: 
   73 00000020              ; 
   74 00000020              ; r2 temporarily holds OEMAddressTable[VA] 
   75 00000020              ; r3 temporarily holds OEMAddressTable[PHY] 
   76 00000020              ; r4 temporarily holds OEMAddressTable[#MB] 
   77 00000020              ; 
   78 00000020 e4912004 25  ldr     r2, [r1], #4                       ; Virtual (cached) address to map physical address to. 
   79 00000024 e4913004     ldr     r3, [r1], #4                       ; Physical address to map from. 
   80 00000028 e4914004     ldr     r4, [r1], #4                       ; Number of MB to map. 
   81 0000002c            
   82 0000002c e3540000     cmp     r4, #0                             ; End of table? 
   83 00000030 0a00000d     beq     %F29 
   84 00000034            
   85 00000034              ; r2 holds the descriptor address (virtual address) 
   86 00000034              ; r0 holds the actual section descriptor 
   87 00000034              ; 
   88 00000034            
   89 00000034              ; Create descriptor address. 
   90 00000034              ; 
   91 00000034 e59f61c8     ldr     r6, =0xFFF00000 
   92 00000038 e0022006     and     r2, r2, r6             ; Only VA[31:20] are valid. 
   93 0000003c e1892922     orr     r2, r9, r2, LSR #18    ; Build the descriptor address:  r2 = (TTB[31:14} | VA[31:20] >> 18) 
   94 00000040            
   95 00000040              ; Create the descriptor. 
   96 00000040              ; 
   97 00000040 e59f61bc     ldr     r6, =0xFFF00000 
   98 00000044 e0033006     and     r3, r3, r6             ; Only PA[31:20] are valid for the descriptor and the rest will be static. 
   99 00000048 e1830000     orr     r0, r3, r0             ; Build the descriptor: r0 = (PA[31:20] | the rest of the descriptor) 
  100 0000004c            
  101 0000004c              ; Store the descriptor at the proper (physical) address 
  102 0000004c              ; 
  103 0000004c e4820004 28  str     r0, [r2], #4 
  104 00000050 e2800601     add     r0, r0, #0x00100000    ; Section descriptor for the next 1MB mapping (just add 1MB). 
  105 00000054 e2444001     sub     r4, r4, #1             ; Decrement number of MB left. 
  106 00000058 e3540000     cmp     r4, #0                 ; Done? 
  107 0000005c 1afffffa     bne     %B28                   ; No - map next MB. 
  108 00000060            
  109 00000060 e3c0020f     bic     r0, r0, #0xF0000000    ; Clear section base address field. 
  110 00000064 e3c006ff     bic     r0, r0, #0x0FF00000    ; Clear section base address field. 
  111 00000068 eaffffec     b       %B25                   ; Get and process the next OEMAddressTable element. 
  112 0000006c            
  113 0000006c              ; ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  114 0000006c              ; ~~~~~~~~~~ MAP UNCACHED and UNBUFFERED SECTION DESCRIPTORS ~~~~~~~~~~~~~~ 
  115 0000006c              ; ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  116 0000006c            
  117 0000006c e3100008 29  tst     r0, #8                 ; Test for 'C' bit set (means we just used  
  118 00000070                                             ; above loop structure to map cached and buffered space). 
  119 00000070 e3c0000c     bic     r0, r0, #0x0C          ; Clear cached and buffered bits in the descriptor (clear C&B bits). 
  120 00000074 e2899b02     add     r9, r9, #0x0800        ; Pointer to the first PTE for "unmapped uncached space" (0x2000 0000 + V_U_Adx). 
  121 00000078 1affffe7     bne     %B20                   ; Repeat the descriptor setup for uncached space (map C=B=0 space). 
